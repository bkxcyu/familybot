<launch>
  <master auto="start"/>

  <!-- TF setting 坐标变换-->
  <include file="$(find project)/launch/includes/art_car_tf.launch.xml" />
  <!--启动雷达-->
  <include file="$(find project)/launch/lidar/rplidar.launch" />
  <!--启动底盘-->
  <node pkg="dji_board" type="dji_board_node.py" name="dji_board_node" output="screen">
  </node>

  <include file="$(find project)/launch/rplidar.launch" />
  <include file="$(find project)/launch/params/slam_gmapping.xml" />
  <include file="$(find project)/launch/params/move_base.xml" />
  <include file="$(find project)/launch/global_map.launch" />
</launch>
